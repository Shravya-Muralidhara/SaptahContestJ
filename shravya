#include <stdio.h>

int main()
{
    int N;
    scanf("%d",&N);
   int values[N][N];
   for(int j=0; j<N; ++j)
   {
   for(int i=0; i<N; ++i)
   {
       scanf("%d", &values[j][i]);
       
   }
   }
   for(int j=0; j<N; ++j)
   {
  
   for(int i=0; i<N; ++i)
   {
       printf("%d \t",values[j][i]);
       
   } 
   printf("\n");
   }
   int max=0;
   int max1=0;
   int a=0;
   for(int j=0; j<N; j++)
   {
       for(int i=0; i<N; i++)
       {
           if(values[j][i]==0)
           a++;
       }
       if(a>max)
       {
           max1=j;
           max=a;
       }
       
   }
   printf("%d",max1);
    return 0;
}
