
#include <stdio.h>

void main()
{
     int a,b,c,x,y,T,t;
     int i=1;
     scanf("%d",&T);
    while(1<=T<=1000)
    {
        if(i<=T)
        {
          scanf("%d",&c);
          if(c==1)
          {
          scanf("%d %d",&x ,&y);
           a=x;
           b=y;
           while(b!=0)
        {
            t=b;
            b=a%b;
            a=t;
        }
        printf("%d",a);
          }
        
          if(c==2)
          {
              scanf("%d %d",&x ,&y);
    a=x;
    b=y;
    while(b!=0)
    {
        t=b;
        b=a%b;
        a=t;
    }
    printf("%d",(x*y)/a);
          }
           i++;
        } 
       
    }
  
}
